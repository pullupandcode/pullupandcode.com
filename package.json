  "scripts": {
    "build": "concurrently \"npm run build:css\" \"remix build\"",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\" \"cd cms && sanity start\"",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@cloudinary/react": "^1.1.0",
    "@cloudinary/url-gen": "^1.6.0",
